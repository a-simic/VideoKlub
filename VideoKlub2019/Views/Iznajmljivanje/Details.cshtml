@model VideoKlub2019.Models.Iznajmljivanje

@{
    ViewData["Title"] = "Details";
}

<h3>Detalji o iznajmljenom filmu:</h3>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            ClanId
        </dt>
        <dd class = "col-sm-10">
            @Model.ClanId
        </dd>
        <dt class="col-sm-2">
            DatumIznajmljivanja
        </dt>
        <dd class = "col-sm-10">
            @Model.DatumIznajmljivanja
        </dd>
        <dt class="col-sm-2">
            DatumVracanja
        </dt>
        <dd class = "col-sm-10">
            @Model.DatumVracanja
        </dd>
        <dt class = "col-sm-2">
            Film
        </dt>
        <dd class = "col-sm-10">
            @Model.Film.Naziv
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.IznajmljivanjeId">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
